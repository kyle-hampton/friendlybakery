<h1>Locations</h1>
<div id="map"></div>
    <script>
      function initMap() {
        var myLatLng = {lat: 40.7831, lng: 73.9712};

        // Create a map object and specify the DOM element
        // for display.
        var map = new google.maps.Map(document.getElementById('map'), {
          center: myLatLng,
          zoom: 4
        });

        // Create a marker and set its position.
        var marker = new google.maps.Marker({
          map: map,
          position: myLatLng,
          title: 'Friendly Bakery'
        });
      }

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBsvDOY7SkR4vC8fjszFbOOuJdKZMhk-FY&callback=initMap"
        async defer></script>
  </body>
